{
  "name": "ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "rm -rf ./dist && tsc -b && npx vite build",
    "rm:comments": "ts-node ./scripts/rm-comments.ts ./src",
    "lint": "eslint .",
    "preview": "npx vite preview",
    "deploy:prod": "npm run build && npx vercel ./dist --yes --no-clipboard --token=$VERCEL_TOKEN --prod"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "bootstrap-icons": "^1.13.1",
    "formik": "^2.4.6",
    "query-string": "^9.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.11",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/axios": "^0.9.36",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-toastify": "^4.0.2",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vercel": "^46.0.2",
    "vite": "^7.1.3"
  }
